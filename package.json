{
  "name": "inialum-mail-service",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "create-token": "node --loader esbuild-register/loader -r esbuild-register ./src/utils/createToken.ts",
    "lint": "eslint --cache \"**/*.{js,jsx,ts,tsx,json}\" --ignore-path .gitignore .",
    "format": "prettier --cache --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "vitest",
    "test:ci": "pnpm run test:coverage",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-sesv2": "^3.449.0",
    "@hono/zod-openapi": "^0.8.3",
    "hono": "^3.9.2",
    "libmime": "^5.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@types/jws": "^3.2.8",
    "@types/libmime": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitest/coverage-v8": "^0.34.6",
    "aws-sdk-client-mock": "^3.0.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jws": "^4.0.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vitest-environment-miniflare": "^2.14.1",
    "wrangler": "^3.15.0"
  }
}
